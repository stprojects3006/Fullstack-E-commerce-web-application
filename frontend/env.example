# Queue-it Configuration
# Set to 'true' to enable Queue-it integration
REACT_APP_QUEUE_IT_ENABLED=true

# Queue-it Account Settings
# Replace with your actual Queue-it credentials
REACT_APP_QUEUE_IT_CUSTOMER_ID=futuraforge
REACT_APP_QUEUE_IT_SECRET_KEY=your-secret-key
REACT_APP_QUEUE_IT_API_KEY=your-api-key

# Development Settings
# Set to 'true' to bypass queue in development
REACT_APP_QUEUE_IT_BYPASS=false

# Debug Settings
# Set to 'true' to enable debug logging
REACT_APP_QUEUE_IT_DEBUG=false

# API Configuration
# Backend API base URL
REACT_APP_API_BASE_URL=http://localhost:8081

# Other Application Settings
# Set to 'production' for production builds
NODE_ENV=development 